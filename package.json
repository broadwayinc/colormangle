{
  "name": "colormangle",
  "version": "0.1.101",
  "description": "ColorMangle creates color scheme, converts color strings to various format and calculates appropriate text color based on contrast ratio.",
  "main": "colormangle.js",
  "browser": "dist/colormangle.js",
  "exports": {
    ".": {
      "require": "./dist/colormangle.cjs",
      "import": "./colormangle.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx bns 3333",
    "build": "npx webpack --config webpack.config.js"
  },
  "keywords": [
    "color",
    "scheme",
    "contrast ratio"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/broadwayinc/colormangle.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/broadwayinc/colormangle/issues"
  },
  "homepage": "https://github.com/broadwayinc/colormangle#readme",
  "devDependencies": {
    "basic-node-server": "^1.0.11",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^4.10.0"
  }
}
